      <TextBox Style="{StaticResource txtEdStyle}" Text="{Binding ReasonWhyMissing, diag:PresentationTraceSources.TraceLevel=High}" ToolTip="" TextWrapping="Wrap" AcceptsReturn="True" VerticalAlignment="Stretch" Height="auto" MaxHeight="60" Tag="ReasonWhyMissing" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" />
