      </CheckBox>
      <CheckBox IsChecked="{Binding AmObligingToKeepUp2DateWithin10Days, diag:PresentationTraceSources.TraceLevel=High}" FlowDirection="LeftToRight" Tag="AmObligingToKeepUp2DateWithin10Days" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
        <TextBlock FlowDirection="LeftToRight" Text="Зобов'язуюсь повідомляти про зміни?" ToolTip="У разі будь-яких змін в інформації, що зазначена в цій анкеті, зобов'язуюся повідомити про них Національний банк України протягом 10-ти днів з дня їх виникнення." Tag="AmObligingToKeepUp2DateWithin10Days" />
      </CheckBox>
      <Expander Header="Підписант" ToolTip="Відомості по особу, що підписала анкету" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="auto" Height="auto" IsExpanded="False" Tag="Signatory" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
        <ContentControl Content="{Binding Signatory, diag:PresentationTraceSources.TraceLevel=High}" Margin="5,2" Tag="Signatory" />
      </Expander>
    </StackPanel>
  </DataTemplate>
</ResourceDictionary>