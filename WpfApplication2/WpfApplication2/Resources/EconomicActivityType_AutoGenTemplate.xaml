      <TextBox Style="{StaticResource txtEdStyle}" Text="{Binding KVEDActivityName, diag:PresentationTraceSources.TraceLevel=High}" ToolTip="" Tag="KVEDActivityName" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" MaxWidth="400" MinWidth="250" HorizontalAlignment="Left" />
